class Union
{
    
    public static ArrayList<Integer> findUnion(int arr1[], int arr2[], int n, int m)
    {
        // add your code here
        TreeSet<Integer> ts=new TreeSet<Integer>();
        for(int i=0;i<n;i++){
            ts.add(arr1[i]);
        }
        for(int j=0;j<m;j++){
            ts.add(arr2[j]);
        }
        ArrayList<Integer> al=new ArrayList<Integer>(ts);
        return al;
    }
}

