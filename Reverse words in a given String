 String reverseWords(String S) {
        // code here 
        String words[]=S.split("\\.");
        
        
        

        StringBuffer sb=new StringBuffer();
        for(int i=words.length-1;i>=0;i--){
            if(i!=0)
                sb.append(words[i]+".");
            else
                sb.append(words[i]);
        }
        return sb.toString();
    }
